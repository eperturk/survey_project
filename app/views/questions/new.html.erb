<h1>Create A New Question for Survey: <%= @survey.title %> </h1>
<%= form_for(@question) do |f| %>
	<%= @new_sequence %>
	<%= f.label :text, "Question Text" %>
	<%= f.text_field :text %>
	<%= f.hidden_field :sequence, :value => @new_sequence %>
	<%= f.hidden_field :survey_id, :value => @question.survey.id %>
	<%= f.submit "Submit" %>
<% end %>